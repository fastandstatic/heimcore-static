<script>jQuery(document).ready(function($) {
        // Obtener el nombre de la p√°gina desde la URL
        var path = window.location.pathname; // Esto devuelve la ruta, por ejemplo, "/nombre-de-la-pagina/"
    		var pageName = path === '/' ? 'home' : path.split('/').filter(function(el) { return el.length !== 0; }).pop(); // Capturar el √∫ltimo segmento o 'home' si es index
    		console.log("Page name:", pageName);

        // Reemplazar caracteres no v√°lidos para nombres de clases CSS (si es necesario)
        var pageClass = pageName.replace(/[^a-z0-9\-]/gi, '-').toLowerCase(); // Convierte el nombre en una clase CSS v√°lida
        console.log("Page class name:", "page-"+pageClass);

        // Agregar la clase al body
        if (pageClass.length > 0) {  // Asegurarse de que haya un nombre de p√°gina
            $('body').addClass('page-' + pageClass); // Agregar clase din√°mica al body
        }

        // Funci√≥n para actualizar el logo seg√∫n el scroll y la clase del body
        function updateLogo() {
            if ($('body').hasClass('noticias-template-default') || $('body').hasClass('page-portal-clientes') || $('body').hasClass('page-heimcore-c-sirt') || $('body').hasClass('page-reportar-incidente') || $('body').hasClass('page-acerca-de-c-sirt') || $('body').hasClass('page-politicas-de-privacidad') || $('body').hasClass('page-customer-portal') || $('body').hasClass('page-report-incident') || $('body').hasClass('page-privacy-policy') || $('body').hasClass('page-about-c-sirt') ) {
                $("#logo_header .elementor-widget-container img").attr("src", "/wp-content/uploads/2024/10/logo_stiky.svg");
            } else {
                if ($(window).scrollTop() > 20) {
                    $("#logo_header .elementor-widget-container img").attr("src", "/wp-content/uploads/2024/10/logo_stiky.svg");
                } else {
                    $("#logo_header .elementor-widget-container img").attr("src", "/wp-content/uploads/2024/09/Logo_heimcore.svg");
                }
            }
        } 

        // Funci√≥n para actualizar el sticky del header seg√∫n el scroll
        function updateStickyHeader() {
            if ($(window).scrollTop() > 20) {
                $("#head").addClass('sticky');
            } else {
                $("#head").removeClass('sticky');
            }
        }
			
				function updateStickyHeaderMovil() {
            if ($(window).scrollTop() > 20) {
                $("#headmovil").addClass('sticky');
            } else {
                $("#headmovil").removeClass('sticky');
            }
        }

        // Primero agregar la clase al body, luego ejecutar las dem√°s funciones
        updateLogo();  // Actualizar el logo inmediatamente al cargar
        updateStickyHeader();  // Actualizar el sticky header inmediatamente al cargar
				updateStickyHeaderMovil();
			
        // Ejecutar las funciones al hacer scroll
        $(window).scroll(function () {
            updateStickyHeader(); 
						updateStickyHeaderMovil(); // Controlar el sticky del header al hacer scroll
            updateLogo();  // Controlar el cambio del logo al hacer scroll
        });
    });</script>

<script>document.addEventListener("DOMContentLoaded", function() {
    const descriptions = document.querySelectorAll('#description_heimcore_post .elementor-widget-container');
    const maxLength = 75;
		
		descriptions.forEach(function(description) {
        console.log('T√≠tulo:', description.textContent); 
    });

    descriptions.forEach(function(description) {
        const text = description.textContent;

        if(text.length > maxLength) {
            const trimmedText = text.substring(0, maxLength) + '...';
            description.textContent = trimmedText;
        }
    });
});</script>

